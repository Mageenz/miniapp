.page-container {
  padding: 0;
  min-height: 100vh;
  background-color: RGBA(243, 243, 243, 1);
}
.head {
  padding: 20rpx 30rpx 22rpx 30rpx;
  background-color: #fff;
}
.good-info {
  padding-right: 20rpx;
  padding-bottom: 30rpx;
  border-bottom: 1rpx rgba(243, 243, 243, 1) solid;
}
.good-info .name {
  margin-bottom: 25rpx;
}
.good-info .image {
  width: 160rpx;
  height: 160rpx;
}
.comment .pf-star, .comment .star-sx{
  width: 120rpx;
  height: 24rpx;
  background-size: 120rpx;
}
.pf-star, .star-sx{
  width: 150rpx;
  height: 30rpx;
  background-size: 150rpx;
  background-repeat: no-repeat;
  background-position: 0;
}
.address {
  padding: 26rpx 20rpx 0 0;
}
.icon-address {
  width: 19rpx;
  height: 25rpx;
  margin-right: 8rpx;
}
.icon-phone {
  width: 46rpx;
  height: 46rpx;
}
.icon-title {
  width: 10rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.body {
  background-color: #fff;
}
.content {
  padding: 30rpx;
}
.content .title {
  margin-bottom: 26rpx;
}
.content .yh {
  height: 100rpx;
  background-image: url(data:image/png;base64,);
  background-size: contain;
  padding: 0 47rpx 0 57rpx;
}
.content.p-b0 {
  padding-bottom: 0;
}
.comment {
  align-items: flex-start;
  padding: 30rpx 0 20rpx 0;
  border-bottom: 1rpx rgba(221, 221, 221, 1) solid;
}
.comment .avator {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}
.comment .good-image {
  width: 150rpx;
  height: 150rpx;
}
.title.m-b10 {
  margin-bottom: 10rpx;
}