.page-btn {
  justify-content: center;
  height: 100rpx;
}
.page-icon {
  width: 24rpx;
  height: 24rpx;
}
.dialog-mask {
  background-color: RGBA(0,0,0,0.4);
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1000;
}
.dialog-content {
  width: 500rpx;
  height: 540rpx;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  background-color: #fff;
  z-index: 1001;
  border-radius: 5rpx;
  padding: 40rpx;
}
.fc {
  height: 113rpx;
  border-bottom:1rpx solid rgba(168,173,175,1);
}
.fc input {
  text-align: center;
  width: 100%;
}
.dialog-btn {
  width: 400rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background:rgba(229,57,53,1);
  border-radius:6px;
  color: #fff;
  margin-top: 40rpx;
  border: none;
}